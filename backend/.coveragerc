[run]
omit =
    tests/*
    __pycache__/*
    */migrations/*
    */alembic/*
    venv/*
    env/*
    */test_*.py
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

